{
  "name": "koa-boilerplate",
  "version": "0.2.0",
  "description": "koa-boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=dev nodemon index.js",
    "start:prod": "pm2 startOrReload ecosystem.config.js --env production",
    "deploy:prod": "pm2 deploy ecosystem.config.js production",
    "test": "NODE_ENV=test mocha test/*.js",
    "start:docker": "pm2-docker ecosystem.config.js --env production",
    "deploy:docker": "docker-compose up -d"
  },
  "dependencies": {
    "config": "^1.26.1",
    "html-minifier": "^3.5.2",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-csrf": "^3.0.5",
    "koa-json": "^2.0.2",
    "koa-log4": "^2.2.1",
    "koa-onerror": "^3.1.0",
    "koa-router": "^7.1.1",
    "koa-session": "^5.0.0",
    "koa-static": "^3.0.0",
    "koa-views": "^6.0.2",
    "moment": "^2.18.1",
    "multer": "^1.3.0",
    "mysql2": "^1.3.6",
    "nunjucks": "^3.0.0",
    "pm2": "^2.4.6",
    "redis-promisify": "^0.1.0",
    "sequelize": "^4.4.2",
    "uuid": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zenxds/koa-seed.git"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.4.1",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  }
}
